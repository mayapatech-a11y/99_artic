    local buttonsContainer = Instance.new("Frame")
    buttonsContainer.Name = "ButtonsContainer"
    buttonsContainer.Size = UDim2.new(1, -20, 0, 50)
    buttonsContainer.Position = UDim2.new(0, 10, 0, 35)
    buttonsContainer.BackgroundTransparency = 1
    buttonsContainer.Parent = linksFrame

    local buttonsGrid = Instance.new("UIGridLayout")
    buttonsGrid.CellSize = UDim2.new(0.5, -5, 0, 25)
    buttonsGrid.CellPadding = UDim2.new(0, 10, 0, 0)
    buttonsGrid.HorizontalAlignment = Enum.HorizontalAlignment.Center
    buttonsGrid.VerticalAlignment = Enum.VerticalAlignment.Center
    buttonsGrid.Parent = buttonsContainer

    -- Discord Button
    local discordButton = Instance.new("TextButton")
    discordButton.Name = "DiscordButton"
    discordButton.Size = UDim2.new(1, 0, 1, 0)
    discordButton.BackgroundColor3 = Color3.fromRGB(88, 101, 242) -- Discord blue
    discordButton.BackgroundTransparency = 0.2
    discordButton.BorderSizePixel = 0
    discordButton.Text = "💬 Discord"
    discordButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    discordButton.TextSize = 11
    discordButton.Font = Enum.Font.GothamBold
    discordButton.Parent = buttonsContainer

    local discordCorner = Instance.new("UICorner")
    discordCorner.CornerRadius = UDim.new(0, 4)
    discordCorner.Parent = discordButton

    -- Updates Button
    local updatesButton = Instance.new("TextButton")
    updatesButton.Name = "UpdatesButton"
    updatesButton.Size = UDim2.new(1, 0, 1, 0)
    updatesButton.BackgroundColor3 = Theme.Accent
    updatesButton.BackgroundTransparency = 0.2
    updatesButton.BorderSizePixel = 0
    updatesButton.Text = "🔄 Updates"
    updatesButton.TextColor3 = Theme.Text
    updatesButton.TextSize = 11
    updatesButton.Font = Enum.Font.GothamBold
    updatesButton.Parent = buttonsContainer

    local updatesCorner = Instance.new("UICorner")
    updatesCorner.CornerRadius = UDim.new(0, 4)
    updatesCorner.Parent = updatesButton

    -- Support Button
    local supportButton = Instance.new("TextButton")
    supportButton.Name = "SupportButton"
    supportButton.Size = UDim2.new(1, 0, 1, 0)
    supportButton.BackgroundColor3 = Theme.Success
    supportButton.BackgroundTransparency = 0.2
    supportButton.BorderSizePixel = 0
    supportButton.Text = "❓ Support"
    supportButton.TextColor3 = Theme.Text
    supportButton.TextSize = 11
    supportButton.Font = Enum.Font.GothamBold
    supportButton.Parent = buttonsContainer

    local supportCorner = Instance.new("UICorner")
    supportCorner.CornerRadius = UDim.new(0, 4)
    supportCorner.Parent = supportButton

    -- Button hover effects
    local function setupButtonEffects(button)
        button.MouseEnter:Connect(function()
            if not isMobile then
                TweenService:Create(button, TweenInfo.new(0.1), {BackgroundTransparency = 0.1}):Play()
            end
        end)
        
        button.MouseLeave:Connect(function()
            if not isMobile then
                TweenService:Create(button, TweenInfo.new(0.1), {BackgroundTransparency = 0.2}):Play()
            end
        end)
        
        button.MouseButton1Click:Connect(function()
            TweenService:Create(button, TweenInfo.new(0.05), {BackgroundTransparency = 0}):Play()
            TweenService:Create(button, TweenInfo.new(0.05), {BackgroundTransparency = 0.2}):Play()
        end)
    end

    setupButtonEffects(discordButton)
    setupButtonEffects(updatesButton)
    setupButtonEffects(supportButton)

    -- Button functionality
    discordButton.MouseButton1Click:Connect(function()
        if scriptData.Discord then
            pcall(function()
                setclipboard(scriptData.Discord)
            end)
            
            -- Show feedback
            local originalText = discordButton.Text
            discordButton.Text = "COPIED!"
            discordButton.BackgroundColor3 = Theme.Success
            
            wait(1.5)
            
            discordButton.Text = originalText
            discordButton.BackgroundColor3 = Color3.fromRGB(88, 101, 242)
        end
    end)

    updatesButton.MouseButton1Click:Connect(function()
        -- Show update information
        local newsText = ""
        for i, item in ipairs(news) do
            newsText = newsText .. "• " .. item .. "\n"
        end
        
        game:GetService("StarterGui"):SetCore("SendNotification", {
            Title = "Latest Updates - v" .. (scriptData.Version or "1.0.0"),
            Text = newsText,
            Duration = 8,
            Icon = "rbxassetid://0"
        })
    end)

    supportButton.MouseButton1Click:Connect(function()
        game:GetService("StarterGui"):SetCore("SendNotification", {
            Title = "Support",
            Text = "Join our Discord for help and support!",
            Duration = 5,
            Icon = "rbxassetid://0"
        })
    end)

    -- Auto-size calculations
    local function updateSizes()
        -- About text auto-size
        local aboutTextSize = game:GetService("TextService"):GetTextSize(
            aboutText.Text, aboutText.TextSize, aboutText.Font, 
            Vector2.new(aboutText.AbsoluteSize.X, math.huge)
        )
        aboutText.Size = UDim2.new(1, 0, 0, aboutTextSize.Y + 10)
        aboutFrame.Size = UDim2.new(1, 0, 0, 40 + aboutTextSize.Y)

        -- Features list auto-size
        local featuresHeight = #features * 20 + (#features - 1) * 6
        featuresList.Size = UDim2.new(1, 0, 0, featuresHeight)
        featuresFrame.Size = UDim2.new(1, 0, 0, 40 + featuresHeight)

        -- News list auto-size
        local newsHeight = 0
        for i, newsItem in ipairs(newsList:GetChildren()) do
            if newsItem:IsA("Frame") then
                local newsText = newsItem:FindFirstChild("NewsText")
                if newsText then
                    local textSize = game:GetService("TextService"):GetTextSize(
                        newsText.Text, newsText.TextSize, newsText.Font, 
                        Vector2.new(newsText.AbsoluteSize.X, math.huge)
                    )
                    newsText.Size = UDim2.new(1, 0, 0, textSize.Y)
                    newsItem.Size = UDim2.new(1, 0, 0, math.max(20, textSize.Y))
                    newsHeight = newsHeight + math.max(20, textSize.Y) + 8
                end
            end
        end
        newsList.Size = UDim2.new(1, 0, 0, newsHeight)
        newsFrame.Size = UDim2.new(1, 0, 0, 40 + newsHeight)

        -- Total container height
        local totalHeight = 70 + (40 + aboutTextSize.Y) + (40 + featuresHeight) + 
                           (40 + newsHeight) + 90 + (infoLayout.Padding.Offset * 4)
        infoContainer.Size = UDim2.new(1, 0, 0, totalHeight)
    end

    -- Update sizes after a brief delay
    spawn(function()
        wait(0.1)
        updateSizes()
    end)

    -- Create script info object with methods
    local scriptInfoObj = {}

    function scriptInfoObj:UpdateDescription(newDescription)
        aboutText.Text = newDescription or aboutText.Text
        updateSizes()
    end

    function scriptInfoObj:UpdateVersion(newVersion)
        version.Text = "v" .. (newVersion or scriptData.Version)
    end

    function scriptInfoObj:AddFeature(featureText)
        local newIndex = #features + 1
        table.insert(features, featureText)
        
        local featureItem = Instance.new("Frame")
        featureItem.Name = "Feature" .. newIndex
        featureItem.Size = UDim2.new(1, 0, 0, 20)
        featureItem.BackgroundTransparency = 1
        featureItem.LayoutOrder = newIndex
        featureItem.Parent = featuresList

        local bullet = Instance.new("TextLabel")
        bullet.Name = "Bullet"
        bullet.Size = UDim2.new(0, 20, 0, 20)
        bullet.Position = UDim2.new(0, 0, 0, 0)
        bullet.BackgroundTransparency = 1
        bullet.Text = "•"
        bullet.TextColor3 = Theme.Success
        bullet.TextSize = 14
        bullet.TextXAlignment = Enum.TextXAlignment.Center
        bullet.Font = Enum.Font.GothamBold
        bullet.Parent = featureItem

        local featureTextLabel = Instance.new("TextLabel")
        featureTextLabel.Name = "FeatureText"
        featureTextLabel.Size = UDim2.new(1, -25, 0, 20)
        featureTextLabel.Position = UDim2.new(0, 20, 0, 0)
        featureTextLabel.BackgroundTransparency = 1
        featureTextLabel.Text = tostring(featureText)
        featureTextLabel.TextColor3 = Theme.TextSecondary
        featureTextLabel.TextSize = 11
        featureTextLabel.TextXAlignment = Enum.TextXAlignment.Left
        featureTextLabel.Font = Enum.Font.Gotham
        featureTextLabel.Parent = featureItem
        
        updateSizes()
    end

    function scriptInfoObj:AddNews(newsItem)
        local newIndex = #news + 1
        table.insert(news, newsItem)
        
        local newsItemFrame = Instance.new("Frame")
        newsItemFrame.Name = "NewsItem" .. newIndex
        newsItemFrame.Size = UDim2.new(1, 0, 0, 0)
        newsItemFrame.BackgroundTransparency = 1
        newsItemFrame.LayoutOrder = newIndex
        newsItemFrame.Parent = newsList

        local newsIcon = Instance.new("TextLabel")
        newsIcon.Name = "Icon"
        newsIcon.Size = UDim2.new(0, 20, 0, 20)
        newsIcon.Position = UDim2.new(0, 0, 0, 0)
        newsIcon.BackgroundTransparency = 1
        newsIcon.Text = "📢"
        newsIcon.TextColor3 = Theme.Accent
        newsIcon.TextSize = 12
        newsIcon.TextXAlignment = Enum.TextXAlignment.Center
        newsIcon.Font = Enum.Font.Gotham
        newsIcon.Parent = newsItemFrame

        local newsText = Instance.new("TextLabel")
        newsText.Name = "NewsText"
        newsText.Size = UDim2.new(1, -25, 0, 0)
        newsText.Position = UDim2.new(0, 25, 0, 0)
        newsText.BackgroundTransparency = 1
        newsText.Text = tostring(newsItem)
        newsText.TextColor3 = Theme.TextSecondary
        newsText.TextSize = 10
        newsText.TextXAlignment = Enum.TextXAlignment.Left
        newsText.TextYAlignment = Enum.TextYAlignment.Top
        newsText.TextWrapped = true
        newsText.Font = Enum.Font.Gotham
        newsText.Parent = newsItemFrame
        
        updateSizes()
    end

    function scriptInfoObj:SetStatus(status, color)
        statusLabel.Text = status:upper()
        statusIndicator.BackgroundColor3 = color or Theme.Success
    end

    function scriptInfoObj:GetInstance()
        return infoContainer
    end

    return scriptInfoObj
end





-- Export the library
return GuiLibrary
