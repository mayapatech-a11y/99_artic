    local newsList = Instance.new("Frame")
    newsList.Name = "NewsList"
    newsList.Size = UDim2.new(1, -20, 0, 0) -- Auto-size
    newsList.Position = UDim2.new(0, 10, 0, 35)
    newsList.BackgroundTransparency = 1
    newsList.Parent = newsFrame

    local newsLayout = Instance.new("UIListLayout")
    newsLayout.Padding = UDim.new(0, 8)
    newsLayout.SortOrder = Enum.SortOrder.LayoutOrder
    newsLayout.Parent = newsList

    -- Create news items
    local news = scriptData.News or {"No recent updates."}
    for i, newsItem in ipairs(news) do
        local newsItemFrame = Instance.new("Frame")
        newsItemFrame.Name = "NewsItem" .. i
        newsItemFrame.Size = UDim2.new(1, 0, 0, 0) -- Auto-size
