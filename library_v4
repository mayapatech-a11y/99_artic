-- Script Info Function (Voidware Style)
function GuiLibrary:CreateScriptInfo(tab, scriptData)
    -- scriptData should contain:
    -- scriptData.Name (string) - Full script name
    -- scriptData.Version (string) - Version number
    -- scriptData.Developer (string) - Developer/Team name
    -- scriptData.Description (string) - About description
    -- scriptData.Features (table) - Array of features
    -- scriptData.News (table) - Array of news/updates
    -- scriptData.Discord (string) - Discord invite link/code
    -- scriptData.Github (string) - GitHub link (optional)
    -- scriptData.LastUpdate (string) - Last update date (optional)

    local infoContainer = Instance.new("Frame")
    infoContainer.Name = "ScriptInfo"
    infoContainer.Size = UDim2.new(1, 0, 0, 0) -- Auto-size
    infoContainer.BackgroundTransparency = 1
    infoContainer.LayoutOrder = 0
    infoContainer.Parent = tab.Frame

    local infoLayout = Instance.new("UIListLayout")
    infoLayout.Padding = UDim.new(0, 12)
    infoLayout.SortOrder = Enum.SortOrder.LayoutOrder
    infoLayout.Parent = infoContainer

    -- Header Section with Voidware styling
    local headerFrame = Instance.new("Frame")
    headerFrame.Name = "Header"
    headerFrame.Size = UDim2.new(1, 0, 0, 70)
    headerFrame.BackgroundColor3 = Theme.Secondary
    headerFrame.BackgroundTransparency = 0.3
    headerFrame.BorderSizePixel = 0
    headerFrame.LayoutOrder = 1
    headerFrame.Parent = infoContainer

    local headerCorner = Instance.new("UICorner")
    headerCorner.CornerRadius = UDim.new(0, 6)
    headerCorner.Parent = headerFrame

    -- Header gradient for Voidware aesthetic
    local headerGradient = Instance.new("UIGradient")
    headerGradient.Color = ColorSequence.new({
        ColorSequenceKeypoint.new(0, Theme.Accent),
        ColorSequenceKeypoint.new(1, Theme.Secondary)
    })
    headerGradient.Rotation = -15
    headerGradient.Transparency = NumberSequence.new({
        NumberSequenceKeypoint.new(0, 0.8),
        NumberSequenceKeypoint.new(1, 0.9)
    })
    headerGradient.Parent = headerFrame

    local scriptIcon = Instance.new("TextLabel")
