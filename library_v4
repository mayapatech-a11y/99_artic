    -- Links Section
    local linksFrame = Instance.new("Frame")
    linksFrame.Name = "LinksSection"
    linksFrame.Size = UDim2.new(1, 0, 0, 90)
    linksFrame.BackgroundColor3 = Theme.Secondary
    linksFrame.BackgroundTransparency = 0.2
    linksFrame.BorderSizePixel = 0
    linksFrame.LayoutOrder = 5
    linksFrame.Parent = infoContainer

    local linksCorner = Instance.new("UICorner")
    linksCorner.CornerRadius = UDim.new(0, 6)
    linksCorner.Parent = linksFrame

    local linksTitle = Instance.new("TextLabel")
    linksTitle.Name = "LinksTitle"
    linksTitle.Size = UDim2.new(1, -20, 0, 25)
    linksTitle.Position = UDim2.new(0, 10, 0, 5)
    linksTitle.BackgroundTransparency = 1
    linksTitle.Text = "🔗 LINKS & SUPPORT"
    linksTitle.TextColor3 = Theme.Text
    linksTitle.TextSize = 12
    linksTitle.TextXAlignment = Enum.TextXAlignment.Left
    linksTitle.Font = Enum.Font.GothamBold
    linksTitle.Parent = linksFrame

    local linksSeparator = Instance.new("Frame")
    linksSeparator.Name = "Separator"
    linksSeparator.Size = UDim2.new(1, -20, 0, 1)
    linksSeparator.Position = UDim2.new(0, 10, 0, 30)
    linksSeparator.BackgroundColor3 = Theme.Accent
